
/* Rules in this file should read as sentences, and should use sentence capitalization. */
/* NOTE: Do NOT translate comparison types (like “is less than” or “begins with”) directly. Please always check English value for the intended usage for the particular case. */

/* Date modified kind rule templates. */
"%[dateModified]@ %[is less than]@ %@" = "%1$[수정한 날짜]@ %2$[이전]@ %3$@";
"%[dateModified]@ %[is greater than]@ %@" = "%1$[수정한 날짜]@ %2$[이후]@ %3$@";

/* Date modified relative rule templates. */
"%[dateModified]@ %[begins with]@ %@" = "%1$[수정한 날짜]@ %2$[과거 항목을 포함함]@ %3$@일";
"%[dateModified]@ %[ends with]@ %@" = "%1$[수정한 날짜]@ %2$[과거 항목을 포함하지 않음]@ %3$@일";

/* Library item kind rule templates. */
"%[kind]@ %[is]@ %[text]@" = "%1$[유형]@ %2$[해당함]@ %3$[텍스트]@";
"%[kind]@ %[is not]@ %[text]@" = "%1$[유형]@ %2$[해당하지 않음]@ %3$[텍스트]@";
"%[kind]@ %[is]@ %[folder]@" = "%1$[유형]@ %2$[해당함]@ %3$[폴더]@";
"%[kind]@ %[is not]@ %[folder]@" = "%1$[유형]@ %2$[해당하지 않음]@ %3$[폴더]@";
"%[kind]@ %[is]@ %[other]@" = "%1$[유형]@ %2$[해당함]@ %3$[기타]@";
"%[kind]@ %[is not]@ %[other]@" = "%1$[유형]@ %2$[해당하지 않음]@ %3$[기타]@";

/* Search rule templates. */
"%[search]@ %[matches]@ %@" = "%1$[검색]@ %2$[일치함]@ %3$@";

/* Library item parent path rule templates. */
"%[parentPath]@ %[is]@ %@" = "%1$[부모 경로]@ %2$[해당함]@ %3$@";
"%[parentPath]@ %[is not]@ %@" = "%1$[부모 경로]@ %2$[해당하지 않음]@ %3$@";

/* Library item ancestor path rule templates. */
"%[ancestorPath]@ %[is]@ %@" = "%1$[조상 경로]@ %2$[해당함]@ %3$@";
"%[ancestorPath]@ %[is not]@ %@" = "%1$[조상 경로]@ %2$[해당하지 않음]@ %3$@";

/* Library item path extension rule templates. */
"%[pathExtension]@ %[is]@ %@" = "%1$[확장명]@ %2$[해당함]@ %3$@";
"%[pathExtension]@ %[is not]@ %@" = "%1$[확장명]@ %2$[해당하지 않음]@ %3$@";
