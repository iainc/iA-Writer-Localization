
/* This file is used to localize smart folder rule editors. Parts of the rules are grouped together. Rules in this file should read as sentences, but each part of the rule must be capitalized as a sentence on its own. */
/* Each part of the rule may include up to three substrings: [^This is a group header][This is a group name][$This is a group footer]. */
/* NOTE: Do NOT translate comparison types (like “is less than” or “begins with”) directly. Please always check English value for the intended usage for the particular case. */

/* Date modified kind rule templates. */
"%[dateModified]@ %[is less than]@ %@" = "%1$[Дата изменения]@ %2$[До даты]@ %3$@";
"%[dateModified]@ %[is greater than]@ %@" = "%1$[Дата изменения]@ %2$[После даты]@ %3$@";

/* Date modified relative rule templates. */
"%[dateModified]@ %[begins with]@ %@" = "%1$[Дата изменения]@ %2$[В последние]@ %3$[^Количество дней]@";
"%[dateModified]@ %[ends with]@ %@" = "%1$[Дата изменения]@ %2$[Не в последние]@ %3$[^Количество дней]@";

/* Library item kind rule templates. */
"%[kind]@ %[is]@ %[text]@" = "%1$[Тип]@ %2$[Только]@ %3$[Текст]@";
"%[kind]@ %[is not]@ %[text]@" = "%1$[Тип]@ %2$[Только не]@ %3$[Текст]@";
"%[kind]@ %[is]@ %[folder]@" = "%1$[Тип]@ %2$[Только]@ %3$[Папка]@";
"%[kind]@ %[is not]@ %[folder]@" = "%1$[Тип]@ %2$[Только не]@ %3$[Папка]@";
"%[kind]@ %[is]@ %[other]@" = "%1$[Тип]@ %2$[Только ]@ %3$[Другой]@";
"%[kind]@ %[is not]@ %[other]@" = "%1$[Тип]@ %2$[Только не]@ %3$[Другой]@";

/* Search rule templates. */
"%[search]@ %[matches]@ %@" = "%1$[Текст]@ %2$[Содержит]@ %3$@";

/* Library item parent path rule templates. */
"%[parentPath]@ %[is]@ %@" = "%1$[Путь к родителю]@ %2$[Совпадает с]@ %3$@";
"%[parentPath]@ %[is not]@ %@" = "%1$[Путь к родителю]@ %2$[Не совпадает с]@ %3$@";

/* Library item ancestor path rule templates. */
"%[ancestorPath]@ %[is]@ %@" = "%1$[Путь к любому из родителей]@ %2$[Совпадает с]@ %3$@";
"%[ancestorPath]@ %[is not]@ %@" = "%1$[Путь к любому из родителей]@ %2$[Не совпадает с]@ %3$@";
