
/* This file is used to localize smart folder rule editors. Parts of the rules are grouped together. Rules in this file should read as sentences, but each part of the rule must be capitalized as a sentence on its own. */
/* Each part of the rule may include up to three substrings: [^This is a group header][This is a group name][$This is a group footer]. */
/* NOTE: Do NOT translate comparison types (like “is less than” or “begins with”) directly. Please always check English value for the intended usage for the particular case. */

/* Date modified kind rule templates. */
"%[dateModified]@ %[is less than]@ %@" = "%1$[Datum geändert]@ %2$[Ist vor]@ %3$@";
"%[dateModified]@ %[is greater than]@ %@" = "%1$[Datum geändert]@ %2$[Ist nach]@ %3$@";

/* Date modified relative rule templates. */
"%[dateModified]@ %[begins with]@ %@" = "%1$[Datum geändert]@ %2$[Ist in der Vergangenheit]@ %3$[^Anzahl Tage]@";
"%[dateModified]@ %[ends with]@ %@" = "%1$[Datum geändert]@ %2$[Ist nicht in der Vergangenheit]@ %3$[^Anzahl Tage]@";

/* Date created kind rule templates. */
"%[dateCreated]@ %[is less than]@ %@" = "%1$[Datum erstellt]@ %2$[Ist vor]@ %3$@";
"%[dateCreated]@ %[is greater than]@ %@" = "%1$[Datum erstellt]@ %2$[Ist nach]@ %3$@";

/* Date created relative rule templates. */
"%[dateCreated]@ %[begins with]@ %@" = "%1$[Datum erstellt]@ %2$[Ist in der Vergangenheit]@ %3$[^Anzahl Tage]@";
"%[dateCreated]@ %[ends with]@ %@" = "%1$[Datum erstellt]@ %2$[Ist nicht in der Vergangenheit]@ %3$[^Anzahl Tage]@";

/* Library item kind rule templates. */
"%[kind]@ %[is]@ %[text]@" = "%1$[Format]@ %2$[Ist]@ %3$[Text]@";
"%[kind]@ %[is not]@ %[text]@" = "%1$[Format]@ %2$[Ist nicht]@ %3$[Text]@";
"%[kind]@ %[is]@ %[folder]@" = "%1$[Format]@ %2$[Ist]@ %3$[Ordner]@";
"%[kind]@ %[is not]@ %[folder]@" = "%1$[Format]@ %2$[Is kein]@ %3$[Ordner]@";
"%[kind]@ %[is]@ %[other]@" = "%1$[Format]@ %2$[Ist]@ %3$[Übrige]@";
"%[kind]@ %[is not]@ %[other]@" = "%1$[Format]@ %2$[Ist nicht]@ %3$[Übrige]@";

/* Search rule templates. */
"%[search]@ %[matches]@ %@" = "%1$[Suche]@ %2$[Entspricht]@ %3$@";

/* Library item parent path rule templates. */
"%[parentPath]@ %[is]@ %@" = "%1$[Elternpfad]@ %2$[Ist]@ %3$@";
"%[parentPath]@ %[is not]@ %@" = "%1$[Elternpfad]@ %2$[Ist nicht]@ %3$@";

/* Library item ancestor path rule templates. */
"%[ancestorPath]@ %[is]@ %@" = "%1$[Vorfahrenpfad]@ %2$[Ist]@ %3$@";
"%[ancestorPath]@ %[is not]@ %@" = "%1$[Vorfahrenpfad]@ %2$[Ist nicht]@ %3$@";
