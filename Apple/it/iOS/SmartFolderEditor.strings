
/* This file is used to localize smart folder rule editors. Parts of the rules are grouped together. Rules in this file should read as sentences, but each part of the rule must be capitalized as a sentence on its own. */
/* Each part of the rule may include up to three substrings: [^This is a group header][This is a group name][$This is a group footer]. */
/* NOTE: Do NOT translate comparison types (like “is less than” or “begins with”) directly. Please always check English value for the intended usage for the particular case. */

/* Date modified kind rule templates. */
"%[dateModified]@ %[is less than]@ %@" = "%1$[Data modifica]@ %2$[È anteriore]@ %3$@";
"%[dateModified]@ %[is greater than]@ %@" = "%1$[Data modifica]@ %2$[È successiva]@ %3$@";

/* Date modified relative rule templates. */
"%[dateModified]@ %[begins with]@ %@" = "%1$[Data modifica]@ %2$[È negli ultimi]@ %3$[^Numero di giorni]@";
"%[dateModified]@ %[ends with]@ %@" = "%1$[Data modifica]@ %2$[Non è negli ultimi]@ %3$[^Numero di giorni]@";

/* Date created kind rule templates. */
"%[dateCreated]@ %[is less than]@ %@" = "%1$[Data creata]@ %2$[È anteriore]@ %3$@";
"%[dateCreated]@ %[is greater than]@ %@" = "%1$[Data creata]@ %2$[È successiva]@ %3$@";

/* Date created relative rule templates. */
"%[dateCreated]@ %[begins with]@ %@" = "%1$[Data creata]@ %2$[È negli ultimi]@ %3$[^Numero di giorni]@";
"%[dateCreated]@ %[ends with]@ %@" = "%1$[Data creata]@ %2$[Non è negli ultimi]@ %3$[^Numero di giorni]@";

/* Library item kind rule templates. */
"%[kind]@ %[is]@ %[text]@" = "%1$[Tipo]@ %2$[È ]@ %3$[Testo]@";
"%[kind]@ %[is not]@ %[text]@" = "%1$[Tipo]@ %2$[Non è]@ %3$[Testo]@";
"%[kind]@ %[is]@ %[folder]@" = "%1$[Tipo]@ %2$[È]@ %3$[Cartella]@";
"%[kind]@ %[is not]@ %[folder]@" = "%1$[Tipo]@ %2$[Non è]@ %3$[Cartella]@";
"%[kind]@ %[is]@ %[other]@" = "%1$[Tipo]@ %2$[È]@ %3$[Altro]@";
"%[kind]@ %[is not]@ %[other]@" = "%1$[Tipo]@ %2$[Non è]@ %3$[Altro]@";

/* Search rule templates. */
"%[search]@ %[matches]@ %@" = "%1$[Cerca]@ %2$[corrispondenti]@ %3$@";

/* Library item parent path rule templates. */
"%[parentPath]@ %[is]@ %@" = "%1$[Percorso padre]@ %2$[È]@ %3$@";
"%[parentPath]@ %[is not]@ %@" = "%1$[Percorso padre]@ %2$[Non è]@ %3$@";

/* Library item ancestor path rule templates. */
"%[ancestorPath]@ %[is]@ %@" = "%1$[Percorso predecessore]@ %2$[È]@ %3$@";
"%[ancestorPath]@ %[is not]@ %@" = "%1$[Percorso predecessore]@ %2$[Non è]@ %3$@";
