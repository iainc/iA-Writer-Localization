
/* This file is used to localize smart folder rule editors. Parts of the rules are grouped together. Rules in this file should read as sentences, but each part of the rule must be capitalized as a sentence on its own. */
/* Each part of the rule may include up to three substrings: [^This is a group header][This is a group name][$This is a group footer]. */
/* NOTE: Do NOT translate comparison types (like “is less than” or “begins with”) directly. Please always check English value for the intended usage for the particular case. */

/* Date modified kind rule templates. */
"%[dateModified]@ %[is less than]@ %@" = "%1$[Date modified]@ %2$[Is before]@ %3$@";
"%[dateModified]@ %[is greater than]@ %@" = "%1$[Date modified]@ %2$[Is after]@ %3$@";

/* Date modified relative rule templates. */
"%[dateModified]@ %[begins with]@ %@" = "%1$[Date modified]@ %2$[Is in the past]@ %3$[^Number of Days]@";
"%[dateModified]@ %[ends with]@ %@" = "%1$[Date modified]@ %2$[Is not in the past]@ %3$[^Number of Days]@";

/* Date created kind rule templates. */
"%[dateCreated]@ %[is less than]@ %@" = "%1$[Date created]@ %2$[Is before]@ %3$@";
"%[dateCreated]@ %[is greater than]@ %@" = "%1$[Date created]@ %2$[Is after]@ %3$@";

/* Date created relative rule templates. */
"%[dateCreated]@ %[begins with]@ %@" = "%1$[Date created]@ %2$[Is in the past]@ %3$[^Number of Days]@";
"%[dateCreated]@ %[ends with]@ %@" = "%1$[Date created]@ %2$[Is not in the past]@ %3$[^Number of Days]@";

/* Library item kind rule templates. */
"%[kind]@ %[is]@ %[text]@" = "%1$[Kind]@ %2$[Is]@ %3$[Text]@";
"%[kind]@ %[is not]@ %[text]@" = "%1$[Kind]@ %2$[Is not]@ %3$[Text]@";
"%[kind]@ %[is]@ %[folder]@" = "%1$[Kind]@ %2$[Is]@ %3$[Folder]@";
"%[kind]@ %[is not]@ %[folder]@" = "%1$[Kind]@ %2$[Is not]@ %3$[Folder]@";
"%[kind]@ %[is]@ %[other]@" = "%1$[Kind]@ %2$[Is]@ %3$[Other]@";
"%[kind]@ %[is not]@ %[other]@" = "%1$[Kind]@ %2$[Is not]@ %3$[Other]@";

/* Search rule templates. */
"%[search]@ %[matches]@ %@" = "%1$[Search]@ %2$[Matches]@ %3$@";

/* Library item parent path rule templates. */
"%[parentPath]@ %[is]@ %@" = "%1$[Parent path]@ %2$[Is]@ %3$@";
"%[parentPath]@ %[is not]@ %@" = "%1$[Parent path]@ %2$[Is not]@ %3$@";

/* Library item ancestor path rule templates. */
"%[ancestorPath]@ %[is]@ %@" = "%1$[Ancestor path]@ %2$[Is]@ %3$@";
"%[ancestorPath]@ %[is not]@ %@" = "%1$[Ancestor path]@ %2$[Is not]@ %3$@";
