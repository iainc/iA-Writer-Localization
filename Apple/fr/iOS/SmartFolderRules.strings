
/* This file is used for localizing smart folder rule summaries. Rules in this file should read as sentences. Use sentence capitalization for the rule a whole. */
/* NOTE: Do NOT translate comparison types (like “is less than” or “begins with”) directly. Please always check English value for the intended usage for the particular case. */

/* Date modified kind rule templates. */
"%[dateModified]@ %[is less than]@ %@" = "%1$[Date de modification]@ %2$[est avant]@ %3$@";
"%[dateModified]@ %[is greater than]@ %@" = "%1$[Date de modification]@ %2$[est après]@ %3$@";

/* Date modified relative rule templates. */
"%[dateModified]@ %[begins with]@ %@" = "%1$[Date de modification]@ %2$[est dans le passé]@ %3$@ jours";
"%[dateModified]@ %[ends with]@ %@" = "%1$[Date de modification]@ %2$[n’est pas dans le passé]@ %3$@ jours";

/* Date created kind rule templates. */
"%[dateCreated]@ %[is less than]@ %@" = "%1$[Date de création]@ %2$[est avant]@ %3$@";
"%[dateCreated]@ %[is greater than]@ %@" = "%1$[Date de la création]@ %2$[est après]@ %3$@";

/* Date created relative rule templates. */
"%[dateCreated]@ %[begins with]@ %@" = "%1$[Date de création]@ %2$[est dans le passé]@ %3$@ jours";
"%[dateCreated]@ %[ends with]@ %@" = "%1$[Date de création]@ %2$[n’est pas dans le passé]@ %3$@ jours";

/* Library item kind rule templates. */
"%[kind]@ %[is]@ %[text]@" = "%1$[Type]@ %2$[est]@ %3$[texte]@";
"%[kind]@ %[is not]@ %[text]@" = "%1$[Type]@ %2$[n’est pas]@ %3$[texte]@";
"%[kind]@ %[is]@ %[folder]@" = "%1$[Type]@ %2$[est]@ %3$[dossier]@";
"%[kind]@ %[is not]@ %[folder]@" = "%1$[Type]@ %2$[n’est pas]@ %3$[dossier]@";
"%[kind]@ %[is]@ %[other]@" = "%1$[Type]@ %2$[est]@ %3$[autre]@";
"%[kind]@ %[is not]@ %[other]@" = "%1$[Type]@ %2$[n’est pas]@ %3$[autre]@";

/* Search rule templates. */
"%[search]@ %[matches]@ %@" = "%1$[Recherche]@ %2$[Correspond à]@ %3$@";

/* Library item parent path rule templates. */
"%[parentPath]@ %[is]@ %@" = "%1$[Chemin ancêtre]@ %2$[est]@ %3$@";
"%[parentPath]@ %[is not]@ %@" = "%1$[Chemin ancêtre]@ %2$[n’est pas]@ %3$@";

/* Library item ancestor path rule templates. */
"%[ancestorPath]@ %[is]@ %@" = "%1$[Chemin ancêtre]@ %2$[est]@ %3$@";
"%[ancestorPath]@ %[is not]@ %@" = "%1$[Chemin ancêtre]@ %2$[n’est pas]@ %3$@";
